<?xml version="1.0" encoding="utf-8"?>


<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="350dp"
        android:layout_height="250dp"
        android:orientation="vertical">


    <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/tvCommandTitle"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/arialmt"
            android:gravity="center_horizontal"
            android:lineSpacingExtra="5.6sp"
            android:text="@string/set_admin_title"
            android:textColor="@color/medium_green"
            android:textSize="16sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.046" />

    <View
            android:layout_width="match_parent"
            android:layout_height="0.7dp"
            android:layout_marginStart="15dp"
            android:layout_marginEnd="15dp"
            android:background="@color/gray14"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.143" />

    <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/etAdminPhone"
            android:layout_width="160.7dp"
            android:layout_height="25dp"
            android:background="@drawable/edit_text_border"
            android:digits="0123456789+-"
            android:imeOptions="actionDone"
            android:layout_marginStart="17.3dp"
            android:ems="12"
            android:maxLines="1"
            android:inputType="phone"
            android:textSize="14.7sp"
            android:textDirection="ltr"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="@+id/tvAdminPhone"
            app:layout_constraintStart_toEndOf="@+id/tvAdminPhone"
            app:layout_constraintTop_toTopOf="@+id/tvAdminPhone" />


    <androidx.appcompat.widget.AppCompatEditText
            android:id="@+id/etPassword"
            android:layout_width="160.7dp"
            android:layout_height="25dp"
            android:background="@drawable/edit_text_border"
            android:imeOptions="actionDone"
            android:ems="12"
            android:inputType="numberPassword"
            android:maxLines="1"
            android:paddingStart="5dp"
            android:paddingEnd="5dp"
            android:textSize="14.7sp"
            android:textDirection="ltr"
            android:textAlignment="center"
            app:layout_constraintBottom_toBottomOf="@+id/tvPassword"
            app:layout_constraintStart_toStartOf="@+id/etAdminPhone"
            app:layout_constraintTop_toTopOf="@+id/tvPassword" />


    <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/btnSendCommand"
            android:layout_width="90dp"
            android:layout_height="31.7dp"
            android:background="@color/green1"
            android:fontFamily="@font/arialmt"
            android:lineSpacingExtra="-9.9sp"
            android:paddingStart="8dp"
            android:paddingEnd="8dp"
            android:text="@string/send"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="18.7sp"
            app:layout_constraintEnd_toEndOf="@+id/etPassword"
            app:layout_constraintTop_toTopOf="@+id/btnCancel" />

    <Button
            android:id="@+id/btnCancel"
            android:layout_width="90dp"
            android:layout_height="31.7dp"
            android:background="@color/red4"
            android:fontFamily="@font/arialmt"
            android:lineSpacingExtra="-9.9sp"
            android:text="@string/cancel"
            android:textAllCaps="false"
            android:textColor="@color/white"
            android:textSize="18.7sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="@+id/tvAdminPhone"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.905" />

    <TextView
            android:id="@+id/tvAdminPhone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="48dp"
            android:fontFamily="@font/arialmt"
            android:lineSpacingExtra="-9.9sp"
            android:text="@string/phone_num"
            android:textColor="@color/black"
            android:textSize="14.7sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.090"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.150" />

    <TextView
            android:id="@+id/tvPassword"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="42.3dp"
            android:fontFamily="@font/arialmt"
            android:lineSpacingExtra="-9.9sp"
            android:text="@string/password_title"
            android:textColor="@color/black"
            android:textSize="14.7sp"
            app:layout_constraintStart_toStartOf="@+id/tvAdminPhone"
            app:layout_constraintTop_toBottomOf="@+id/tvAdminPhone" />


</androidx.constraintlayout.widget.ConstraintLayout>


